<div class="category__panel">
    <div class="category__panel-inner">
        <ul class="category__menu">
            <% categories.forEach(category => { %>
                <li class="category__menu-item txt_color_white">
                    <a class="category__menu-item--link" href="<%= category.url %>"><%= category.name %></a>
                </li>
            <% }) %>
        </ul>
    </div>
</div>